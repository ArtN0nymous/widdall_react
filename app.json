{
  "expo": {
    "name": "widdall",
    "slug": "widdall",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./src/img/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "googleServicesFile":"./google-services.json",
      "package": "com.widdall",
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "extra": {
      "eas": {
        "projectId": "50d56be2-dbf8-48bf-bcb1-cd32814d846d"
      }
    }
  },
  "plugins": [
    [
      "expo-notifications",
      {
        "icon": "./src/img/icon.png",
        "androidMode": "default",
        "color": "#ffffff",
        "sounds": [
          "./src/assets/sounds/notification.wav"
        ],
        "androidCollapsedTitle": "Widdall"
      }
    ]
  ]
}
