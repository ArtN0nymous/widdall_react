{
  "expo": {
    "name": "widdall",
    "slug": "widdall",
    "version": "1.1.2",
    "orientation":"portrait",
    "icon":"./src/img/icon.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.widdall",
      "versionCode":1,
      "useNextNotificationsApi": true
    }
  },
  "plugins": [
    [
      "expo-notifications",
      {
        "icon": "./src/img/icon.png",
        "androidMode": "default",
        "color": "#ffffff",
        "sounds": [
          "./src/assets/sounds/notification.wav"
        ],
        "androidCollapsedTitle": "Widdall"
      }
    ]
  ]
}
